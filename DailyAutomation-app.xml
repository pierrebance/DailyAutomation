<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/2.5">
    <id>DailyAutomation</id>
    <versionNumber>1.0.0</versionNumber>
    <filename>DailyAutomation</filename>
    <description>Deploy flash templates and commands with versioning</description>
    <!-- To localize the description, use the following format for the description element.<description><text xml:lang="en">English App description goes here</text><text xml:lang="fr">French App description goes here</text><text xml:lang="ja">Japanese App description goes here</text></description>-->
    <name>DailyAutomation</name>
    <!-- To localize the name, use the following format for the name element.<name><text xml:lang="en">English App name goes here</text><text xml:lang="fr">French App name goes here</text><text xml:lang="ja">Japanese App name goes here</text></name>-->
    <copyright></copyright>
    <initialWindow>
        <content>DailyAutomation.swf</content>
        <systemChrome>standard</systemChrome>
        <transparent>false</transparent>
        <visible>true</visible>
        <fullScreen>false</fullScreen>
        <aspectRatio>portrait</aspectRatio>
        <renderMode>auto</renderMode>
        <maximizable>false</maximizable>
        <minimizable>true</minimizable>
        <resizable>false</resizable>
    </initialWindow>
    <icon>
        <image16x16>icons/logo16.png</image16x16>
        <image32x32>icons/logo32.png</image32x32>
        <image48x48>icons/logo48.png</image48x48>
        <image128x128>icons/logo128.png</image128x128>
    </icon>
    <customUpdateUI>false</customUpdateUI>
    <allowBrowserInvocation>false</allowBrowserInvocation>
</application>